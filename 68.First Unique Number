class FirstUnique:
    def __init__(self, nums):
        self.queue = []
        self.count = {}
        for num in nums:
            self.add(num)

    def showFirstUnique(self):
        while self.queue and self.count[self.queue[0]] > 1:
            self.queue.pop(0)
        return self.queue[0] if self.queue else -1

    def add(self, value):
        if value in self.count:
            self.count[value] += 1
        else:
            self.count[value] = 1
            self.queue.append(value)
